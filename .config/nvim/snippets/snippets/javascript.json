{
  "describe": {
    "prefix": "desc",
    "body": "describe('${1:description}', () => {\n\t${0}\n})"
  },
  "it": {
    "prefix": "it",
    "body": "it('${1:description}', async () => {\n\t${0}\n})"
  },
  "beforeAll": {
    "prefix": "ba",
    "body": "beforeAll(async () => {\n\t${0}\n})"
  },
  "beforeEach": {
    "prefix": "bfe",
    "body": "beforeEach(async () => {\n\t${0}\n})"
  },
  "export all": {
    "prefix": "export-all",
    "body": "export * from './${0}';"
  },
  "class": {
    "prefix": "class",
    "body": "class ${1:name} {\n\tconstructor (${2:arguments}) {\n\t\t${0}\n\t}\n}"
  },
  "import module": {
    "prefix": "import",
    "body": "import {${2:*}} from '${1:module}'"
  },
  "try/catch": {
    "prefix": "try",
    "body": "try {\n\t${0}\n} catch (${1:err}) {\n\t\n}"
  },
  "function": {
    "prefix": "func",
    "body": "function ${1:name} (${2:arguments}) {\n\t${0}\n}"
  },
  "arrow function": {
    "prefix": "fn",
    "body": "(${1}) => ${0}"
  },
  "arrow function with body": {
    "prefix": "fnb",
    "body": "(${1}) => {\n\t${0}\n}"
  },
  "console.log": {
    "prefix": "log",
    "body": "console.log(${0})"
  },
  "constructor": {
    "prefix": "constructor",
    "body": "constructor () {\n\t${0}\n}"
  },
  "method": {
    "prefix": "method",
    "body": "${1:method} (${2:arguments}) {\n\t${0}\n}"
  },
  "For-Of Loop": {
    "prefix": "forof",
    "body": ["for (const ${1:iterator} of ${2:object}) {", "\t$0", "}"],
    "description": "For-Of Loop"
  },
  "If Statement": {
    "prefix": "if",
    "body": ["if (${1:condition}) {", "\t$0", "}"],
    "description": "If Statement"
  },
  "If-Else Statement": {
    "prefix": "ifelse",
    "body": ["if (${1:condition}) {", "\t$0", "} else {", "\t", "}"],
    "description": "If-Else Statement"
  },
  "While Statement": {
    "prefix": "while",
    "body": ["while (${1:condition}) {", "\t$0", "}"],
    "description": "While Statement"
  }
}
